{
  "routes": [
    {
      "route": "/",
      "rewrite": "/index.html"
    }
  ],
  "mimeTypes": {
    ".json": "application/json",
    ".gz": "application/gzip"
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/new_data.gz"]
  },
  "responseOverrides": {
    "/new_data.gz": {
      "headers": {
        "Content-Encoding": "gzip",
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    }
  }
}
